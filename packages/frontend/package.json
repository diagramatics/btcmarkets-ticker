{
  "name": "btcmarkets-ticker-frontend",
  "version": "0.3.0",
  "description": "Frontend for btcmarkets-ticker",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "test": "npm run lint && preact test",
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "serve": "npm run build && preact serve",
    "dev": "preact watch",
    "deploy": "npm run build && now ./build --name btcmarkets-ticker -e NOW_SH=true --public && now alias -r rules.json"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "babel-plugin-styled-components": "^1.3.0",
    "eslint": "^4.12.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "if-env": "^1.0.0",
    "preact-cli": "^2.1.0",
    "prettier": "^1.8.2"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "localforage": "^1.5.3",
    "moment": "^2.19.2",
    "preact": "^8.2.6",
    "preact-compat": "^3.17.0",
    "preact-router": "^2.6.0",
    "prop-types": "^15.6.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-persist": "^5.4.0",
    "styled-components": "^2.2.3"
  }
}
